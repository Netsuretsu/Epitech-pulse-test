AA;AAAA;AAAA;;AASV,iCAAW;AACTC,yBAAO,CAACC,GAAR;AACA,uBAAI,CAACT,OAAL,GACE,wDADF;AAEF,iBAJA,MAIO;AACL,uBAAI,CAACA,OAAL,GAAe,sBAAf;AACF;;AAfU;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAiBb;AAlBM;AAZI,CAAf","names":["name","components","data","username","password","message","methods","login","trim","$store","dispatch","$router","push","console","log"],"sourceRoot":"","sources":["/home/netsuretsu/Desktop/ecp/src/components/LoginComp.vue"],"sourcesContent":["<template>\n  <div class=\"form-container\">\n    <p v-if=\"message\" class=\"text-red\">{{ message }}</p>\n\n    <form @submit.prevent=\"login\">\n      <q-input\n        ref=\"username\"\n        :rules=\"[(val) => !!val || 'Username is required']\"\n        v-model=\"username\"\n        class=\"q-mt-sm\"\n        outlined\n        label=\"Username\"\n      />\n\n      <q-input\n        ref=\"password\"\n        :rules=\"[(val) => !!val || 'Password is required']\"\n        type=\"password\"\n        v-model=\"password\"\n        class=\"q-mt-sm\"\n        outlined\n        label=\"Password\"\n      />\n\n      <div class=\"q-mt-md\">\n        <q-btn class=\"q-ml-sm\" color=\"positive\" type=\"submit\"> Login </q-btn>\n      </div>\n    </form>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"LoginComp\",\n  components: {},\n\n  data() {\n    return {\n      username: \"\",\n      password: \"\",\n      message: \"\",\n    };\n  },\n\n  methods: {\n    async login() {\n      try {\n        const data = {\n          username: this.username.trim(),\n          password: this.password.trim(),\n        };\n        await this.$store.dispatch(\"login\", data);\n        this.$router.push(\"/\");\n      } catch (error) {\n        if (error) {\n          console.log(error);\n          this.message =\n            \"Invalid credentials or network error! Please try again\";\n        } else {\n          this.message = \"Failed to logged in!\";\n        }\n      }\n    },\n  },\n};\n</script>\n"]}]}